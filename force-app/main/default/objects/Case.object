    <validationRules>
        <fullName>Department_users_cannot_edit_the_status</fullName>
        <active>true</active>
        <description>Department Requestor can&apos;t change Transmittal Document Status</description>
        <errorConditionFormula>OR(
IF(
ISNEW(),
IF(
AND(
$RecordType.Name &lt;&gt; &apos;Goldenrod&apos;,
$RecordType.Name &lt;&gt; &apos;Constituent Concern&apos;,
$Profile.Name = &apos;Department User&apos;,
Text(Status) &lt;&gt; &apos;Draft&apos;
),
True,
False ) ,False),

AND(
$Profile.Name = &apos;Department User&apos;,
NOT(ISBLANK(TEXT( Status ))),
ISCHANGED(Status),
$RecordType.Name &lt;&gt; &apos;Goldenrod&apos;,
$RecordType.Name &lt;&gt; &apos;Constituent Concern&apos;,
$RecordType.Name &lt;&gt; &apos;Governor’s Office Action Request (GOAR)&apos;,
$RecordType.Name &lt;&gt; &apos;Governor’s Office Action Request (GOAR)-Legal&apos;,
$RecordType.Name &lt;&gt; &apos;Governor’s Office Action Request (GOAR)-Scheduling&apos;
),

AND(
$Profile.Name = &apos;Department User&apos;,
NOT(ISBLANK(TEXT( Status ))),
ISCHANGED(Status),
ISPICKVAL(PRIORVALUE(Status),&apos;Approved by CalSTA&apos;),
OR (TEXT( Status ) = &apos;Draft&apos;, TEXT( Status ) = &apos;Pending at Agency&apos;, TEXT( Status ) = &apos;Pending at Department for Revision&apos;),
OR ($RecordType.Name = &apos;Governor’s Office Action Request (GOAR)&apos;,
$RecordType.Name = &apos;Governor’s Office Action Request (GOAR)-Legal&apos;,
$RecordType.Name = &apos;Governor’s Office Action Request (GOAR)-Scheduling&apos;)
),

AND(
$Profile.Name = &apos;Department User&apos;,
NOT(ISBLANK(TEXT( Status ))),
ISCHANGED(Status),
OR(ISPICKVAL(PRIORVALUE(Status),&apos;Draft&apos;),
ISPICKVAL(PRIORVALUE(Status),&apos;Pending at Agency&apos;),
ISPICKVAL(PRIORVALUE(Status),&apos;Pending at Department for Revision&apos;)),
OR ($RecordType.Name = &apos;Governor’s Office Action Request (GOAR)&apos;,
$RecordType.Name = &apos;Governor’s Office Action Request (GOAR)-Legal&apos;,
$RecordType.Name = &apos;Governor’s Office Action Request (GOAR)-Scheduling&apos;)
))</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Department Requestor can&apos;t change Transmittal Document Status</errorMessage>
