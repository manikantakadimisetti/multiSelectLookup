        <errorDisplayField>Issue_Memo_Type__c</errorDisplayField>
        <errorMessage>This field is non-editable</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Confidential_Field_Access</fullName>
        <active>true</active>
        <description>Confidential Field Acess Can not have  a edit access for once submit for approval</description>
        <errorConditionFormula>AND(!ISPICKVAL(Approve_Status__c , &apos;Not Started&apos;),ISCHANGED( Confidential__c ))</errorConditionFormula>
        <errorDisplayField>Confidential__c</errorDisplayField>
        <errorMessage>This field is non-editable</errorMessage>
