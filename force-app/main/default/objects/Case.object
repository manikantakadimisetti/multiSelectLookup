    </validationRules>
    <validationRules>
        <fullName>Agency_user_when_status_pending_at_dep</fullName>
        <active>true</active>
        <description>Agency users should not be able to change the status when status is “Pending at department”</description>
        <errorConditionFormula>AND( 

$Profile.Name = &quot;Agency User&quot;,
$RecordType.Name = &quot;Goldenrod&quot;,
 ISPICKVAL(PRIORVALUE(Status),&quot;Pending at Department&quot;),
    
       ISCHANGED( Status )
   
       
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Agency users cannot change the status when status is “Pending at Department”</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_Edits</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
